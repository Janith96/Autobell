    Private Sub Form1_Load(ByVal sender As Object, ByVal e As EventArgs) Handles MyBase.Load

        'timer detect

        If System.IO.File.Exists("D:\Autobell\act_sch.abl") = True Then
            Dim strFile As String = "D:\Autobell\act_sch.abl"
            Dim sr As New IO.StreamReader(strFile)
            If sr.ReadLine() = "Default" Then
                autobell_timer.Enabled = True
                s_song_timer.Enabled = False
                PictureBox2.Visible = True
                sr.Close()
            ElseIf sr.ReadLine() = "Custom" Then
                autobell_timer.Enabled = False
                s_song_timer.Enabled = True
                PictureBox3.Visible = True
                sr.Close()
            End If
        Else
            autobell_timer.Enabled = True
            s_song_timer.Enabled = False
            If System.IO.File.Exists("D:\Autobell\time.abl") = True And System.IO.File.Exists("D:\Autobell\track_config.abl") = True Then
                PictureBox2.Visible = True
            End If
        End If

        'folder check
        If System.IO.Directory.Exists("D:\Autobell") = False Then
            Dim filepath As String = "D:\Autobell"
            System.IO.Directory.CreateDirectory(filepath)

            If System.IO.Directory.Exists("D:\Autobell\Audio files") = False Then
                Dim filepath2 As String = "D:\Autobell\Audio files"
                System.IO.Directory.CreateDirectory(filepath2)
            End If
        End If

        'Password check
        If System.IO.File.Exists("D:\Autobell\config.abl") = False Then
            Me.Visible = False
            Form22.Visible = True
        ElseIf System.IO.File.Exists("D:\Autobell\config.abl") = True Then
            Dim strFile As String = "D:\Autobell\config.abl"
            Dim sr As New IO.StreamReader(strFile)
            If sr.ReadLine() = "" Then
                Me.Visible = False
                Form22.Visible = True
            End If

        End If

        'time and task check
        If System.IO.File.Exists("D:\Autobell\time.abl") = False Or System.IO.File.Exists("D:\Autobell\track_config.abl") = False Then
            mtm_notf_text.Visible = True
            mtm_notf_ico.Visible = True
        Else

            mtm_notf_text.Visible = False
            mtm_notf_ico.Visible = False
        End If


    End Sub

